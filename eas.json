{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_SERVER_URL": "https://ytapp.zenextech.in",
        "GOOGLE_SERVICES_FILE": "./SERVICE_KEY/google-services.json"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
